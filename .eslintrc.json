{
  "extends":[ "next/core-web-vitals", "airbnb"],
  "rules": {
    // ...
    "no-unused-vars": "warn",
    "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx", ".tsx"] }],
    "import/extensions": ["error", "ignorePackages", {
      "js": "never",
      "jsx": "never",
      "ts": "never",
      "tsx": "never"
    }]
  }
}

